  <PropertyGroup>
    <PreBuildEvent>@echo off
setlocal enabledelayedexpansion

cd "$(ProjectDir)..\Binaries\RubyMin\"

for /f "delims=" %25%25A in ('dir "$(ProjectDir)\Resources\*.rb" /b/s') do (
	set inputFilePath=%25%25A
	set inputFileExtension=!inputFilePath:~-7!
	set outputFileExtension=.min.rb
	set outputFilePath=!inputFilePath:~0,-3!!outputFileExtension!
	
	if /i not !inputFileExtension!==!outputFileExtension! (
		del "!outputFilePath!"
		start /b/wait RBMin.exe "!inputFilePath!" -o "!outputFilePath!" -k
	)
)</PreBuildEvent>
  </PropertyGroup>
